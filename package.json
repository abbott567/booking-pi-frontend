{
  "name": "booking-pi-frontend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">6.0.0"
  },
  "scripts": {
    "test": "xo && mocha src/test"
  },
  "dependencies": {
    "body-parser": "^1.16.1",
    "compression": "^1.6.2",
    "date-fns": "^1.27.2",
    "express": "^4.14.1",
    "got": "^6.7.1",
    "govuk-elements-marko": "^2.4.2",
    "govuk-elements-sass": "^2.2.1",
    "govuk-template-marko": "^0.19.2",
    "jquery": "^1.12.4",
    "lasso": "~2.10.4",
    "lasso-babel-transform": "^0.4.0",
    "lasso-sass": "^1.1.9",
    "marko": "^3.14.2",
    "marko-i18next": "^0.2.0",
    "moment": "^2.17.1",
    "reset-css": "^2.2.0",
    "socket.io": "^1.7.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "mocha": "^3.2.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "env": [
      "node",
      "mocha",
      "browser",
      "jquery"
    ],
    "ignores": [
      "**/*.marko.js"
    ],
    "overrides": [
      {
        "files": "src/pages/room/socket-functions.js",
        "esnext": false
      }
    ],
    "rules": {
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}

{
  "plugins": [
    {
      "plugin": "lasso-sass",
      "config": {
        "outputStyle": "compressed",
        "includePaths": [
          "./node_modules/govuk_frontend_toolkit/stylesheets",
          "./node_modules/govuk-elements-sass/public/sass"
        ]
      }
    }
  ],
  "outputDir": "static",
  "fingerprintsEnabled": true,
  "minify": true,
  "resolveCssUrls": true,
  "bundlingEnabled": true
}
